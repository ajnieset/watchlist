html
  head
    title= "Watchlist"
  body
    h1= "Movies"
    a(type="button" href="movies/add") Add Movie
    ul
      each movie in watchlist 
        li= movie.title 
        a(type="button" href=`/movies/view/${movie._id}`) Edit
        form(action=`/movies/${movie._id}` method="POST")
          input(type="submit" value="Delete")
        
        